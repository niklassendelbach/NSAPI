@model IEnumerable<NSApp.Models.MemberInterest>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.URL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Members)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Interests)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.URL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Members.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Interests.Title)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MemberInterestId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MemberInterestId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MemberInterestId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
